[Global]
username = {{ username }}
password = {{ password }}
domain-id = {{ domain_id }} 
auth-url = {{ auth_url }}
tenant-id = {{ tenant_id }}
region = RegionOne

[LoadBalancer]
use-octavia=true
subnet-id={{ kubeadm_os_subnet_id }}
floating-network-id={{ kubeadm_os_floating_network_id }}
max-shared-lb={{ kubeadm_os_max_shared_lb }}
lb-provider=octavia

[BlockStorage]
bs-version=v2